<div class="page-header">
  Log In Here!
</div>

<%= render partial: "partial/flash" %>

<div class="form">
  <%= form_tag login_path do %>
    <div class="form-group">
      <%= label_tag :username %>:
      <%= text_field_tag :username %>
    </div>

    <div class="form-group">
      <%= label_tag :password %>:
      <%= password_field_tag :password %>
    </div>

    <div class="button">
      <%= submit_tag "Log In" %>
    </div>
  <% end %>
</div>
